if test \! -d inst
then
    mkdir inst
fi

if test \! -d inst/include
then
    mkdir inst/include
fi

if test \! -d inst/src
then
    mkdir inst/src
fi

if test \! -d inst/doc
then
    mkdir inst/doc
fi

cp include/* inst/include
cp src/demo*.cpp src/Makefile.* inst/src
cp doc/rcppbind.pdf inst/doc
cp LICENSE.txt inst
